package com.shopme.common.entity;

import java.util.HashSet;
import java.util.Set;

import javax.persistence.Entity;
import javax.persistence.Table;


@Entity
@Table(name="brands")
public class Brand {
	private Integer id;
	
	private String name;
	
	private String logo;
	
	private Set<Category> categories = new HashSet<>();
	
}
