import java.util.HashMap;
import java.util.Set;

public class Category {
	private Integer id;
	
	private String name;
	
	private String alias;
	
	private String image;
	
	private boolean enabled;
	
	private Category parent;
	
	private Set<Category> children = new HashSet<>();
	
	
}
